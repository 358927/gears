			HitSound:Play()
			penguinMaker = script.PenguinMaker
			penguinMaker.Parent = hit.Parent
			penguinMaker.Disabled = false
			connection:disconnect()
			if thrownPP then
				thrownPP:Remove()
			end
		end
	end
end

-- Connect to the touched event!
if thrownPP then
	connection = thrownPP.Touched:connect(onTouched)
end

