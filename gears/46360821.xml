function onJump()
	--if walkAnim then
		--walkAnim:Stop()
	--end
	JumpAnim = Tool:FindFirstChild("Humanoid"):LoadAnimation(script.PenguinJump)
	JumpAnim:Play()
	print("PENGUIN JUMP")
	wait(0.4)
	JumpAnim:Stop()
	--walkAnim:Play()
	Tool:FindFirstChild("Humanoid").WalkSpeed = 8.0
	wait(0.7)
	--walkAnim:Stop()
	Tool:FindFirstChild("Humanoid").WalkSpeed = 16.0
end

