	Services.Input:SetTitle("Primary","Close Portals")
	Services.Input:SetPosition("Primary",UDim2.new(.5,0,-.5,0))
end

function Unequipped()
	Services.Input:UnbindAction("Primary")
end

Tool.Equipped:Connect(Equipped)
Tool.Unequipped:Connect(Unequipped)


