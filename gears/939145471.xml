	elseif v[1]:IsA("CharacterMesh") or v[1]:IsA("DataModelMesh") or v[1]:IsA("Clothing") or v[1]:IsA("ShirtGraphic") or v[1]:IsA("Decal") then
		v[1]:Destroy()
		v[3].Parent = v[2]
	end
end

Humanoid.WalkSpeed = OriginalWalkSpeed
Sparkles:Destroy()

