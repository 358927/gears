	creator_tag.Name = "creator"
	creator_tag.Parent = bomb

	bomb.Parent = game.Workspace
	local newScript = Tool.BlastScript:clone()
	newScript.Disabled = false
	newScript.Parent = bomb
end


Tool.Enabled = true
