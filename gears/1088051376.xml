		Gear.Class:BindLungeAnimation(NewAnimation)
	else
		local NewAnimation = Instance.new("Animation")
		NewAnimation.AnimationId = "rbxassetid://1088118628"
		Gear.Class:BindLungeAnimation(NewAnimation)
	end
