function blow(hit)
	if hit and hit.Parent then
		local humanoid = hit.Parent:FindFirstChild("Humanoid")
		if humanoid and myHumanoid and humanoid ~= humanoid then
			humanoid:TakeDamage(15)
		end
		local fire = Instance.new("Fire")
		fire.Parent = hit
		debris:AddItem(fire, 5)
	end
end

