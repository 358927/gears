			v:Destroy()
		end
	end
end

Humanoid.Changed:connect(function(Property)
	if Property == "Health" then
		if Humanoid.Health &lt; Health and not IgnoreChange then
			local HealthLost = (Health - Humanoid.Health)
			IgnoreChange = true
			UntagHumanoid(Humanoid)
			TagHumanoid(Humanoid, Creator.Value)
			Humanoid:TakeDamage(HealthLost)
			Health = Humanoid.Health
			IgnoreChange = false
		elseif Humanoid.Health &gt;= Health then
			Health = Humanoid.Health
		end
	elseif Property == "Jump" then
		Humanoid.Jump = false
	end
end)

