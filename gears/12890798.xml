}

Grips = {
	Normal = CFrame.new(0, 0, -0.4, 0, 0, 1, 1, 0, 0, 0, 1, 0),
	Active = CFrame.new(0.5, -1.5, -1.56, 0, 0, -1, -0.707, -0.707, -0, -0.707, 0.707, 0),
}

