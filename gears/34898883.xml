			firstWeld = true
			if hit.Anchored then
				ball.Anchored = true
				ball.CFrame = cf
			else
				ball.CFrame = cf
				newWeld = Instance.new("Weld")
				newWeld.Name = "Glue"
				newWeld.Part0 = hit
				newWeld.Part1 = ball
				newWeld.C0 = hit.CFrame:toObjectSpace(cf)
				newWeld.Parent = game.Workspace
			end

			HitSound:Play()

			for index, sc in next, reporters do sc:Destroy() end
		end
	end
end

