part = Instance.new("Part")
part.Size = Vector3.new(0.8, 0.8, 0.8)
part.Transparency = 0.0
part.CanCollide = false

local fieldMesh = Instance.new("SpecialMesh")
fieldMesh.MeshType = "FileMesh"
