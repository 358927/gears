		if (script.Dir.Value.magnitude &gt; .9 and script.Dir.Value.magnitude &lt; 1.1) then
			direction = script.Dir.Value
		end
		spread(direction)
		wait(pauseTime)
		burnSound:Play()
	end
	wait(1)
	if burnSound then burnSound:Stop() end
	wait(reload)
	Tool.Enabled = true
end

