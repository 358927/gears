Arrow.Anchored = true
Arrow.CanCollide = false
Arrow.Name = 'Arrow'

local ArrowMesh = Instance.new('SpecialMesh')
ArrowMesh.Parent = Arrow
ArrowMesh.Scale = Vector3.new(8, 8, 8)
ArrowMesh.MeshId = 'http://www.roblox.com/asset/?id=110309063'
