	RightMotor.C0 = MotorC0
	Sounds.Laser:Stop()
	if CurrentLaser then
		CurrentLaser:Destroy()
	end
	CurrentLaser = nil
end

function Activated()
	index = (Colors[index+1] and index+1) or 1
	if CurrentLaser then
		CurrentLaser.BrickColor = BrickColor.new(Colors[index])
	end
end

