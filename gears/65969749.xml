	stillEquipped = true
	if mouse == nil then
		print("Mouse not found")
		return
	end
	while stillEquipped do
		print("Setting Mouse to go")
		mouse.Icon = "rbxasset://textures\\GunCursor.png"
		while Tool.Enabled and stillEquipped do
